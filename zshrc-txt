## usage: review xxx xxx ...
function review() {
    names=("廖建" "程庆林" "方圆" "黄力" "马赢" "王加闯" "王然" "王天鹏")
    phones=("13810611450" "15867166146" "18500614668" "18105170912" "13311110199" "15321719302" "18101244062" "18190993448")
    i=$(($RANDOM % 8))
    if [[ $SHELL == "/bin/zsh" ]]; then
		i=`expr $i + 1`
	fi
    name=${names[$i]}
    phone=${phones[$i]}
    msg="恭喜 $name review\n$*"
    data="{ \"msgtype\": \"text\", \"text\": { \"content\": \"$msg\", \"mentioned_mobile_list\":[\"$phone\"] } }"
    echo $data
    curl 'https://qyapi.weixin.qq.com/cgi-bin/webhook/send?key=f214b3d6-7774-4f9a-b8bf-3a0f1df8e107' -H 'Content-Type: application/json' -d "$data"
}
